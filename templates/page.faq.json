    "b580a5ec-04b2-46fb-9acf-feb6427ec1c7": {
      "type": "faq",
      "blocks": {
        "d92cb292-41e2-4102-abc1-cbf29ffa287a": {
          "type": "faq",
          "settings": {
            "title": "What is your return/exchange policy?",
            "title_richtext": "",
            "answer": "<p>P+H wants our customers to be completely happy with their purchase. We will accept returns within thirty (30) days of order placement for a full refund. Goods must be returned unused in original packaging. Return shipping charges will be deducted from the original form of payment. All Sale products are final sale.<br/><br/>If you need to return an item, please <a href=\"https://porteandhall.loopreturns.com/#/\" title=\"https://porteandhall.loopreturns.com/#/\">click here</a>. A return label will be provided to you. If you experience any issues getting a return label please email us at <a href=\"mailto:support@porteandhall.com\" target=\"_blank\" title=\"mailto:support@porteandhall.com\">support@porteandhall.com</a></p>"
          }
        },
        "fe9fe020-398e-4749-a79a-2e3a023e2ee8": {
          "type": "faq",
          "settings": {
            "title": "How do I return an item?",
            "title_richtext": "",
            "answer": "<p>Please visit our <a href=\"https://porteandhall.loopreturns.com/#/\" title=\"https://porteandhall.loopreturns.com/#/\">returns portal</a>. Enter your order number and the email address used to place the order. A return label will be generated and further instructions will be provided. If you experience any issues with our returns portal please reach out to <a href=\"mailto:support@porteandhall.com\" target=\"_blank\" title=\"mailto:support@porteandhall.com\">support@porteandhall.com</a>.</p>"
          }
        },
        "8dcc265d-06ae-4904-b547-d384e9930daa": {
          "type": "faq",
          "settings": {
            "title": "Do you charge a return fee? What is the return fee?",
            "title_richtext": "",
            "answer": "<p>Return shipping fees are due to extremely high shipping costs that our carriers have imposed on us. The return shipping fee is only a fraction of the actual shipping cost. The return shipping fees range from $10-$35 depending on the product you are returning. We do not charge any other restocking fees.</p>"
          }
        }
      },
