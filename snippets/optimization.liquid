{{ 'lazysizes.min.js' | asset_url | preload_tag: as: 'script' }}
{{ 'swiper-bundle.min.js' | asset_url | preload_tag: as: 'script' }}
{{ 'micromodal.min.js' | asset_url | preload_tag: as: 'script' }}
{{ 'gsap.min.js' | asset_url | preload_tag: as: 'script' }}
{{ 'ScrollToPlugin.min.js' | asset_url | preload_tag: as: 'script' }}
{{ 'global.js' | asset_url | preload_tag: as: 'script' }}
{{ 'custom.js' | asset_url | preload_tag: as: 'script' }}

<!-- handwriting -->
<link rel="preload" href="{{ 'LaBelleAurore-Regular.ttf' | asset_url }}" as="font" type="font/ttf" crossorigin>

<!-- louize -->
<link rel="preload" href="{{ 'LouizeDisplayTrial-Regular.woff' | asset_url }}" as="font" type="font/woff" crossorigin>
<link rel="preload" href="{{ 'LouizeDisplayTrial-Medium.woff' | asset_url }}" as="font" type="font/woff" crossorigin>

<!-- brandon light -->
<link rel="preload" href="{{ 'BrandonGrotesque-Light.woff2' | asset_url }}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ 'BrandonGrotesque-Light.woff' | asset_url }}" as="font" type="font/woff" crossorigin>

<!-- brandon regular -->
<link rel="preload" href="{{ 'BrandonGrotesque-Regular.woff2' | asset_url }}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ 'BrandonGrotesque-Regular.woff' | asset_url }}" as="font" type="font/woff" crossorigin>

<!-- brandon regular italic -->
<link rel="preload" href="{{ 'BrandonGrotesque-RegularItalic.woff2' | asset_url }}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ 'BrandonGrotesque-RegularItalic.woff' | asset_url }}" as="font" type="font/woff" crossorigin>

<!-- brandon medium -->
<link rel="preload" href="{{ 'BrandonGrotesque-Medium.woff2' | asset_url }}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ 'BrandonGrotesque-Medium.woff' | asset_url }}" as="font" type="font/woff" crossorigin>

<!-- brandon bold -->
<link rel="preload" href="{{ 'BrandonGrotesque-Bold.woff2' | asset_url }}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ 'BrandonGrotesque-Bold.woff' | asset_url }}" as="font" type="font/woff" crossorigin>

{% if collection and collection.products.size > 0 %}
  <link rel="preload" as="image"
    href="{{ collection.products.first.featured_image | img_url: '1000x' }}"
    imagesrcset="
      {{ collection.products.first.featured_image | img_url: '750x' }} 750w,
      {{ collection.products.first.featured_image | img_url: '1000x' }} 1000w"
    imagesizes="100vw">
{% endif %}
