{% schema %}
  {
    "name": "Custom Collection",
    "class": "custom-collection-section",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Heading",
        "default": "Title"
      }
    ],
    "blocks": [
    {
     "type": "block",
     "name": "Collection-col",
     "limit":4,
     "settings": [
        {
          "type": "text",
          "id": "title",
          "label": "Enter type"
        },
       {
          "type": "header",
          "content": "Product 1"
        },
         {
          "type": "text",
          "id": "badge1",
          "label": "Enter Badge"
        },
        
       {
          "type": "header",
          "content": "Product 2"
        },
         {
          "type": "text",
          "id": "badge2",
          "label": "Enter Badge"
        }
      ]
    }
  
  ],
    "presets": [
    {
      "name": "Custom Collection",
      "settings": {}
    }
  ]
  }
{% endschema %}

